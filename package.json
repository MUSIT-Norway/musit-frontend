    "react-dom": "^15.0.0",
    "react-fontawesome": "^1.2.0",
    "react-i18nify": "^1.6.0",
    "react-loader": "^2.4.0",
    "react-marked": "^0.3.1",
    "react-redux": "^4.4.5",
